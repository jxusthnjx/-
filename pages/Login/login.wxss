.wrapperm {
  width: 100%;
  height: 400rpx;
  background-color: rgb(14, 147, 46);
  position: absolute;
}

.loginformm {
  height: 400rpx;
  position: relative;
  top: 200rpx;
}

#owl-loginm {
  width: 400rpx;
  height: 200rpx;
  position: absolute;
  top: -182rpx;
  left: 50%;
  margin-left: -200rpx;
}

.headm{
  width: 400rpx;
  height: 200rpx;
}

.handm{
  width: 68rpx;
  height: 68rpx;
  border-radius: 80rpx;
  background-color: #472d20;
  transform: scaleY(0.6);
  transition: 0.3s ease-out;
  position: absolute;
  left: 25rpx;
  bottom: -16rpx;
}

.hand-rm{
  left: 315rpx;
}

.armsm{
  top: 120rpx;
  position: absolute;
  width: 100%;
  height: 80rpx;
  overflow: hidden;
}

.armm{
  width: 80rpx;
  height: 130rpx;
  position: absolute;
  left: 40rpx;
  top: 80rpx;
  transition: 0.3s ease-out;
  transform: rotate(-20deg);
}

.arm-rm{
  transform: rotate(20deg) scaleX(-1);
  left: 290rpx;
}

.hand-passwordm{
  transform: translateX(95rpx) translateY(-30rpx) scale(0.7);
}

.hand-r-passwordm{
  transform: translateX(-90rpx) translateY(-30rpx) scale(0.7);
}

.arm-passwordm{
  transform: translateY(-80rpx) translateX(80rpx);
}

.arm-r-passwordm{
  transform: translateY(-80rpx) translateX(-72rpx) scaleX(-1);
}

.control-groupm{
  width: 700rpx;
  height: 400rpx;
  margin: 0 auto;
  padding: 20rpx;
  background-color: white;
  border: 1px solid #ddd;
}

.controlm{
  width: 650rpx;
  height: 80rpx;
  margin: 20rpx auto;
  padding: 15rpx;
  font-size: 40rpx;
  border: 1px solid #ddd;
  border-radius: 10rpx;
}

.buttonm{
  margin-top: 40rpx;
}


.infom{
  margin-top: 60px;
  margin-left: 30px;
  margin-right: 30px;
  color: #472d20;
  font-size: 14px;
}