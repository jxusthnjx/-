page {
  background: #ebebeb;
}

.index_list {
  color: #666;
  padding-top: 20rpx;
}

navigator {
  background: #fff;
  margin-bottom: 30rpx;
  padding: 40rpx 40rpx;
  overflow: hidden;
  border-bottom: 1px solid #ededee;
  border-top: 1px solid #ededee;
}

.list_header_left image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 10rpx;
  float: left;
  border-radius: 40rpx;
  overflow: hidden;
}

.list_header view {
  float: left;
  line-height: 80rpx;
  color: #1e1e1e;
  font-size: 16px;
}

.list_header  .list_header_right {
  float: right;
  color: #999;
  font-size: 12px;
}

.list_header {
  padding-bottom: 18rpx;
  overflow: hidden;
}

.listPic {
  width: 100%;
  height: 360rpx;
  margin-bottom: 16rpx;
}

.listTitle {
  font-size: 18px;
  color: #1e1e1e;
  margin-bottom: 16rpx;
  margin-bottom: 16rpx;
}

.listContent {
  line-height: 19px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #646464;
  font-size: 16px;
  margin-bottom: 16rpx;
  max-height: 36px;
}

.list_footer {
  float: right;
  overflow: hidden;
  height: 60rpx;
  line-height: 81rpx;
  margin-top: -8px;
}

.list_footer view {
  float: left;
  padding-left: 44rpx;
  padding-right: 16rpx;
  background: url("http://bmob-cdn-17114.b0.upaiyun.com/2018/08/12/f02db58840797f3b803f98f574d68111.png") no-repeat left 18rpx;
  background-size: 44rpx auto;
  color: #999;
}

.list_footer .commNum {
  background-image: url("http://bmob-cdn-17114.b0.upaiyun.com/2018/08/12/184c178740c01e0e8077199eaace42a0.png");
}

.list_footer .agree_1 {
  background-image: url("http://bmob-cdn-17114.b0.upaiyun.com/2018/08/12/d93abfe240c14dc58007a16011bef503.png");
  color: #c0935b;
}

.loading {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2;
  background: #000;
}
.returnTop{
  width:100rpx;
  height: 100rpx;
  background: url("../../images/returntop.png") no-repeat;
  padding: 0;
  background-size: cover;
  position: fixed;
  right: 74rpx;
  bottom: 100rpx;
  border-bottom: 0;
  border-top: 0;
}

.toWrite {
  width: 100rpx;
  height: 100rpx;
  background: url(http://bmob-cdn-17114.b0.upaiyun.com/2018/08/14/c3d7995f401683b980181acd44bda590.png) no-repeat;
  padding: 0;
  background-size: cover;
  position: fixed;
  right: 74rpx;
  bottom: 100rpx;
  border-bottom: 0;
  border-top: 0;
}

